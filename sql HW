3.1
Task
CREATE OR REPLACE FUNCTION getSysdate
return date 
is 
    l_sysdate date;
begin
    select sysdate into l_sysdate from dual;
return l_sysdate;
end;
/
select getSysdate() from dual;

Task
CREATE OR REPLACE FUNCTION getlength
    return NUMBER 
IS
    rowlength NUMBER;
BEGIN 
    select count(*) INTO rowlength from mediatype;
    return rowlength;
END;
/
SELECT getlength() from mediatype;


