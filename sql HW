select max(unitprice) from track;

3.3
Task
CREATE OR REPLACE FUNCTION getavg
    return NUMBER 
IS
    avgprice NUMBER;
BEGIN 
    select (unitprice*quantity) INTO avgprice from invoiceline;
    return avgprice;
END;
/
3.4
Task
select * from employee where birthdate > '31-DEC-67' INTO birthday;

7.1
Task
SELECT customer.firstname || customer.lastname AS name, invoice.invoiceid
FROM customer
INNER JOIN invoice ON customer.customerid = invoice.invoiceid;

7.2
Task
SELECT  customer.customerid as id, customer.firstname || customer.lastname as name, invoice.invoiceid as invoice, invoice.total as total FROM customer
LEFT OUTER JOIN invoice on customer.customerid = invoice.customerid;

7.3
Task
SELECT album.title, artist.name FROM artist RIGHT OUTER JOIN album ON ALBUM.artistid = artistid.artistid;

7.4
Task
SELECT * FROM album CROSS JOIN artist ORDER BY artist.name;
7.5
Task
SELECT first.employed, first.lastname || first.firstname AS name, second.REPORTSTO FROM employee first, employee second WHERE first.employeed = second.reporsto; 







