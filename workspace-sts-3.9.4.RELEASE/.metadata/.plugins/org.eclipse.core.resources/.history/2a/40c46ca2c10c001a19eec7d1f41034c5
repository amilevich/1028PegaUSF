package com.homework.algorthim;

import java.util.ArrayList;

public class Q12EnhancedLoop {
	int n = 100;
	ArrayList<Integer> evenNum = new ArrayList<Integer>(n);

	public void enhancedLoop() {
		for(int n : evenNum) {
			evenNum.add(n);
			System.out.println(evenNum);			
		}
		
	}
	
	
	
