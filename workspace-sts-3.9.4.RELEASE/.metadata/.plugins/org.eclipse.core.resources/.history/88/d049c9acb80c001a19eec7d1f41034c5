package com.homework.algorthim;

import java.util.ArrayList;

public class Q9PrimeNumbers {
	ArrayList<Integer> primeN = new ArrayList<Integer>();	
	static int count = 0;
	public void primeNum() {
		
		for(int i = 0; i <=100; i++) {
			System.out.println(i);
		}
		
		for(int j = 2; j <=100; j++) {
			count = 0;
			for(int k =1; k<=j; k++) {
				if(k%j==0) {
					count++;
				}
				if (count == 2) {
					System.out.println(k+" ");
				}
			}
		}
	}
}
