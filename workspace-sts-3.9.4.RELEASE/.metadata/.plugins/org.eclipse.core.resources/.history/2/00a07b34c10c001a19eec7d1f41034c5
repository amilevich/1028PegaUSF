package com.homework.algorthim;

public class Q12EnhancedLoop {

	public void enhancedLoop() {
		int n =100;
		int[] arr = new int[n];
		
		for(int i = 0; i < arr.length; i++) {
			if(i  %  2 == 1) {
				arr[i] = i+1;
				System.out.println(arr[i]);				
			}else {
				System.out.println("odd");
			}
			
		}
	}
}
